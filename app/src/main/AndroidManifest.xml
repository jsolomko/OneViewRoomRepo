<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oneviewroomapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OneViewRoomApp">
        <activity
            android:name=".ui.CustomPushActivity"
            android:exported="false" />
        <activity android:name=".ui.DrawerBaseActivity" />
        <activity android:name=".ui.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.SettingActivity" />
        <activity android:name=".ui.PushActivity" />
        <activity android:name=".ui.MainActivity">

        </activity>
    </application>

</manifest>